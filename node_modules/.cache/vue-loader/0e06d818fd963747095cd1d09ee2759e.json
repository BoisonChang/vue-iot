           this.$store.commit(\"setSortStatus\", { \r\n                    sortByDevId: this.sortByDevId,\r\n                    sortByStatus: this.sortByStatus,\r\n                    sortByLow: this.sortByLow,                                         \r\n                });\r\n                this.toolbarStatus = false\r\n            },\r\n            sortByDevIdOrder(){\r\n                this.sortByDevId = !this.sortByDevId\r\n                this.sortByStatus = !this.sortByStatus\r\n            },\r\n            sortByStatusOrder(){\r\n                this.sortByDevId = !this.sortByDevId\r\n                this.sortByStatus = !this.sortByStatus\r\n            },\r\n            sortByHighOrder(){\r\n                this.sortByLow = false\r\n            },\r\n            sortByLowOrder(){\r\n                this.sortByLow = true\r\n            }, \r\n        },\r\n        watch:{\r\n            data(){\r\n                this.usedata = this.data\r\n            },\r\n            originaldata(){\r\n                this.apidata = this.originaldata\r\n            }\r\n        },\r\n        mounted: function(){\r\n            this.$nextTick(function() {\r\n                document.addEventListener(\"click\", e => { \r\n                    this.detectClick(e);    \r\n                });\r\n            });\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .curtain {\r\n        position: absolute;\r\n        /* width: calc(100% - 30px); */\r\n        width: 49%;\r\n        background-color: #fff;\r\n        -webkit-box-shadow: rgb(0 0 0 / 20%) 0px 8px 16px 0px;\r\n        box-shadow: rgb(0 0 0 / 20%) 0px 8px 16px 0px;\r\n        padding: 0px 16px;\r\n        z-index: 999;\r\n    }\r\n    .curtain .curtain-body {\r\n        border-top: 1px solid #dee2e6;\r\n        border-bottom: 1px solid #dee2e6;\r\n        max-height: 350px;\r\n        overflow-y: auto;\r\n    }\r\n    .curtain .curtain-head {\r\n        padding: 0.25rem;\r\n    }\r\n    .curtain .curtain-head .btn-curtain-close {\r\n        background: #24AEDF;\r\n        color: #fff;\r\n        border: 0;\r\n        width: 30px;\r\n        \r\n    }\r\n    .curtain .curtain-clssify {\r\n        text-align: left;\r\n        padding: 1rem;\r\n    }\r\n    .curtain-clssify{\r\n        \r\n    }\r\n    .curtain .curtain-clssify .curtain-clssify-body {\r\n        padding-top: 1rem;\r\n        padding-bottom: 1rem;\r\n        \r\n    }\r\n    .curtain .curtain-filter.disabled, .curtain .curtain-filter:disabled {\r\n        opacity: 0.45;\r\n        color: #5F5F5F !important;\r\n        background-color: #C4C4C4 !important;\r\n        border: none !important;\r\n        \r\n    }\r\n    .curtain .curtain-filter {\r\n        background: #24AEDF;\r\n        color: #fff;\r\n        /* font-weight: bold; */\r\n        border-radius: 18px;\r\n        margin: 0.25rem;\r\n        min-width: calc(25% - 1rem);\r\n        border: none;\r\n        word-break: break-word;\r\n        -webkit-box-sizing: border-box;\r\n        box-sizing: border-box;\r\n        display: inline-block;\r\n        text-align: center;\r\n        white-space: nowrap;\r\n        vertical-align: middle;\r\n        -webkit-user-select: none;\r\n        -moz-user-select: none;\r\n        -ms-user-select: none;\r\n        user-select: none;\r\n        padding: 3px 16px;\r\n        font-size: 1rem;\r\n        line-height: 1.5;\r\n        -webkit-transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;\r\n        transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;\r\n        transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;\r\n        transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;\r\n    }  \r\n    .curtain .curtain-foot {\r\n        text-align: center;\r\n        padding: 1rem;\r\n    }    \r\n    .dropdown_border-left {\r\n        border-left: 5px solid #FFCB00;\r\n    }\r\n    body .primary-bg {\r\n        background: #7EDBCD;\r\n    }\r\n    body .tertiary-bg {\r\n        background: #4ACBD6;\r\n    }\r\n    .btn-group > .btn:first-child {\r\n        border-top-left-radius: 50px;\r\n        border-bottom-left-radius: 50px;\r\n    }\r\n    .btn-white {\r\n        color: #23595E;\r\n        background-color: #fff;\r\n        border-color: #F6F7FB;\r\n        -webkit-box-shadow: 0px 4px 12px rgb(0 0 0 / 8%);\r\n        box-shadow: 0px 4px 12px rgb(0 0 0 / 8%);\r\n    }\r\n    .btn-white.disabled, .btn-white:disabled {\r\n        color: #C4C4C4;\r\n        background-color: #fff;\r\n        border-color: #ECECEC;\r\n    }\r\n    .btn-group > .btn:last-child {\r\n        border-top-right-radius: 50px;\r\n        border-bottom-right-radius: 50px;\r\n    }\r\n    .btn-white:hover {\r\n        color: #fff;\r\n        background-color: #4ACBD6;\r\n        border-color: #4ACBD6;\r\n    }\r\n</style>"]}]}