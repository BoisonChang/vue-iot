rops){\r\n        const store = useStore()\r\n        const devId = ref(true)\r\n        const devIdSave = ref(true)\r\n        const status = ref(false)\r\n        const statusSave = ref(false)\r\n        const low = ref(true)\r\n        const lowSave = ref(true)\r\n        const toolbarStatus = ref(false)\r\n        const apidata = ref({})\r\n        const addToolbar = ref(null)\r\n        const addToolbarBtn = ref(null)\r\n\r\n        const toggleToolbar = () => {\r\n            toolbarStatus.value = !toolbarStatus.value\r\n            keepNowOrder()\r\n        }\r\n        const detectClick = (e) => {\r\n            if (addToolbar.value && !addToolbar.value.contains(e.target) && !addToolbarBtn.value.contains(e.target) ) {\r\n                toolbarStatus.value = false;\r\n                keepNowOrder()\r\n            }\r\n        }\r\n        const sortItem = () => {\r\n            store.commit(\"setSortStatus\", { \r\n                sortByDevId: devId.value,\r\n                sortByStatus: status.value,\r\n                sortByLow: low.value,                                         \r\n            });\r\n            toolbarStatus.value = false\r\n            saveNewOrder()\r\n        }\r\n        const sortByDevIdOrder = () => {\r\n            devId.value = true\r\n            status.value = false\r\n        }\r\n        const sortByStatusOrder = () => {\r\n            devId.value = false\r\n            status.value = true\r\n        }\r\n        const sortByHighOrder = () => {\r\n            low.value = false\r\n        }\r\n        const sortByLowOrder = () => {\r\n            low.value = true\r\n        }\r\n        const resetOrder = () => {\r\n            devId.value = true\r\n            status.value = false\r\n            low.value = true \r\n        }\r\n        const keepNowOrder = () => {\r\n            devId.value = devIdSave.value\r\n            status.value = statusSave.value\r\n            low.value = lowSave.value\r\n        }\r\n        const saveNewOrder = () => {\r\n            devIdSave.value =  devId.value\r\n            statusSave.value = status.value\r\n            lowSave.value = low.value\r\n        }\r\n        watch(props.originaldata, () => {\r\n            apidata.value = props.originaldata.value\r\n        })\r\n        onMounted(()=>{\r\n            nextTick(function() {\r\n                document.addEventListener(\"click\", e => { \r\n                    detectClick(e);    \r\n                });\r\n            });   \r\n        })\r\n        return {\r\n            devId,\r\n            devIdSave,\r\n            status,\r\n            statusSave,\r\n            low,\r\n            lowSave,\r\n            toggleToolbar,\r\n            toolbarStatus,\r\n            sortItem,\r\n            sortByDevIdOrder,\r\n            sortByStatusOrder,\r\n            sortByHighOrder,\r\n            sortByLowOrder,\r\n            resetOrder,\r\n            addToolbar,\r\n            addToolbarBtn\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\nbody {\r\n\t.primary-bg {\r\n\t\tbackground: #7EDBCD;\r\n\t}\r\n\t.tertiary-bg {\r\n\t\tbackground: #4ACBD6;\r\n\t}\r\n}\r\n.curtain {\r\n\tposition: absolute;\r\n\twidth: 49%;\r\n\tbackground-color: #fff;\r\n\t-webkit-box-shadow: rgb(0 0 0 / 20%) 0px 8px 16px 0px;\r\n\tbox-shadow: rgb(0 0 0 / 20%) 0px 8px 16px 0px;\r\n\tpadding: 0px 16px;\r\n\tz-index: 999;\r\n\t.curtain-body {\r\n\t\tborder-top: 1px solid #dee2e6;\r\n\t\tborder-bottom: 1px solid #dee2e6;\r\n\t\tmax-height: 350px;\r\n\t\toverflow-y: auto;\r\n\t\t.curtain-clssify-head {\r\n\t\t\tborder-bottom: 1px solid #dee2e6;\r\n\t\t}\r\n\t}\r\n\t.curtain-head {\r\n\t\tpadding: 0.25rem;\r\n\t\t.btn-curtain-close {\r\n\t\t\tbackground: #24AEDF;\r\n\t\t\tcolor: #fff;\r\n\t\t\tborder: 0;\r\n\t\t\twidth: 30px;\r\n\t\t}\r\n\t}\r\n\t.curtain-clssify {\r\n\t\ttext-align: left;\r\n\t\tpadding: 1rem;\r\n\t\t.curtain-clssify-body {\r\n\t\t\tpadding-top: 1rem;\r\n\t\t\tpadding-bottom: 1rem