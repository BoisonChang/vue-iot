ick=\"sortByDevIdOrder\" :class=\"{ disabled: status }\"  type=\"button\" class=\"curtain-filter \" > 機器編號 </button>\r\n                            <button v-on:click=\"sortByStatusOrder\" :class=\"{ disabled: devId }\" type=\"button\" class=\"curtain-filter \" > 機器狀態</button>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"curtain-clssify-head fs-18 text-left py-3\">\r\n                        <label class=\"curtain-class-label align-middle mb-0 tertiary-color\"><span class=\"dropdown_border-left pe-3\"></span>順序高低</label>\r\n                    </div>\r\n                    <div class=\"curtain-clssify\">\r\n                        <div class=\"curtain-clssify-body\">\r\n                            <button v-on:click=\"sortByHighOrder\" :class=\"{ disabled: low }\"  type=\"button\" class=\"curtain-filter\"><i class=\"fas fa-sort-amount-down\"></i> 由高至低</button>\r\n                            <button v-on:click=\"sortByLowOrder\" :class=\"{ disabled: !low }\" type=\"button\" class=\"curtain-filter\"><i class=\"fas fa-sort-amount-up\"></i> 由低至高</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"curtain-foot\">\r\n                    <button v-on:click=\"resetOrder\" type=\"button\" class=\"btn primary-bg btn-lg rounded-0 text-white mx-2\">重設</button>\r\n                    <button v-on:click=\"sortItem\" type=\"button\" class=\"btn tertiary-bg btn-lg rounded-0 text-white mx-2\">套用</button>\r\n                </div>\r\n            </div></span>\r\n\r\n            <button type=\"button\" class=\"btn btn-white btn-sort mx-3 mx-md-0\" v-on:click=\"toggleToolbar\" ref=\"addToolbarBtn\">\r\n                <i class=\"fas fa-sort-amount-down pe-2\"></i>\r\n                <span>排序方式</span>\r\n            </button>\r\n        </div>  \r\n    </div>\r\n</template>\r\n<script>\r\nimport { onMounted, ref, watch, nextTick, defineProps} from 'vue'\r