tent":["<template>\r\n    <div class=\"mobileTitle col-12 d-block d-lg-none py-3\">\r\n        <div class=\"row mx-3 my-2\">\r\n            <div class=\"col-12 pl-0\">\r\n                <label class=\"title\">\r\n                    <span data-i18n=\"monitoringDevice\">裝置監控</span>\r\n                    <p class=\"title_border border_blue mt-4\"></p>\r\n                </label>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"row middle-bar \">\r\n        <!-- [section]search-bar -->\r\n        <div class=\"col-md-6 col-sm-5 col-12\">\r\n            <div class=\"card card-effect mx-3 mx-md-0 h-100\">\r\n                <div class=\"card-body py-3 card-search\">\r\n                    <label class=\"d-flex px-2 mb-0\">\r\n                        <i class=\"fas fa-search p-2 me-4\"></i> <input v-model=\"search\"  type=\"search\" class=\"form-control form-control-sm\">\r\n                    </label>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!--更新倒數 START-->\r\n        <div class=\"col-md-6 col-sm-5 col-12\">\r\n            <div class=\"card card-effect mx-3 mx-md-0 time-card h-100\">\r\n                <div class=\"card-body py-3 card-count\">\r\n                    <div class=\"row d-flex px-2 mb-0\">\r\n                        <div class=\"col-md-6 col-5 d-flex justify-content-center  align-items-center\">\r\n                            <label for=\"m_timer\" class=\"fw-bold\">更新倒數</label>\r\n                        </div>\r\n                        <div class=\"col-md-6 col-5 \">\r\n                            <div ><span class=\"timerColor style colorDefinition size_md\" id=\"m_timer\">00:{{ count }}</span></div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>   \r\n    <!-- <h2> {{ search }} </h2>  -->\r\n</template>\r\n\r\n<script>\r\nimport { onMounted, ref, watch } from 'vue'\r\nimport { useStore } from \"vuex\";\r\nexport default {\r\n    name: 'middlebar',\r\n    emits: ['pageKeyUpdate'] ,\r\n    setup( props, { emit }){\r\n        const store = useStore()\r\n        const search = ref('')\r\n        const count = ref(15)\r\n        const pageKey = ref(null)\r\n        const countDown = () => {\r\n            setInterval(() => {\r\n                count.value --\r\n                if(count.value === 0){\r\n                    pageKey.value = new Date().getTime()\r\n                    emit(\"pageKeyUpdate\", pageKey) // ''\r\n                    count.value = 15\r\n                } \r\n                if(count.value < 10){\r\n                    count.value = '0'+ count.value\r\n                }            \r\n            }, 1000)\r\n        }\r\n        watch(search, () => {\r\n            store.commit(\"setSearchStatus\", search);\r\n        })\r\n        onMounted(()=>{\r\n            countDown()      \r\n        })\r\n        return {\r\n            count,\r\n            countDown,\r\n            search\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .middle-bar{\r\n        margin-top: 20px;\r\n    }\r\n    .card-search{\r\n        border-left: 6px solid #24AEDF;\r\n    }\r\n    .card-count{\r\n        border-left: 6px solid #FFCB00;\r\n    }\r\n    .timerColor {\r\n        background: transparent !important;\r\n        color: #067a8f !important;\r\n        border-color: transparent !important;\r\n        width: 100%;\r\n        font-family: sans-serif;\r\n        font-size: 28px;\r\n        font-weight: bold;\r\n        border-style: solid;\r\n    }\r\n    .border_blue {\r\n        border-color: #24AEDF;\r\n    }\r\n    .title {\r\n        font-weight: 900;\r\n        font-size: 24px;\r\n        line-height: 24px;\r\n        letter-spacing: 0.06em;\r\n        text-shadow: 0px 4px 4px rgb(0 0 0 / 25%);\r\n    }\r\n    .title_border {\r\n        border-bottom: 3px solid;\r\n        width: 47.5px;\r\n        -webkit-margin-start: 0% !important;\r\n        margin-inline-start: 0% !important;\r\n    }\r\n\r\n@media screen and (max-width: 768px){\r\n    #m_timer {\r\n        font-size: 18px;\r\n    }\r\n}\r\n\r\n@media screen and (max-width: 576px){\r\n    .time-card {\r\n        margin-top: 1rem;\r\n        white-space:nowrap;\r\n    }\r\n    body .sideBar_container, .d-xs-none {\r\n        display: none;\r\n    }\r\n    #m_timer {\r\n        font-size: 28px;\r\n    }\r\n}\r\n\r\n\r\n\r\n</style>"]}]}