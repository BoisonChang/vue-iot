agination class=\"pagination mt-5\" :show-all-page-number=\"false\" @afterDataUpdate=\"afterDataUpdate\" :axiosConfigOpts=\"axiosConfigOpts\" :serverSide=\"true\" :key=\"pageKey\" />    \r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from 'vue'\r\nimport { useStore } from \"vuex\";\r\nimport middlebar from '@/components/middlebar'\r\nimport filterbar from '@/components/filterbar'\r\nimport devicecard from '@/components/devicecard'\r\nimport FPagination from '@/components/comm/FPagination';\r\n\r\nexport default {\r\n    name: 'App',\r\n        components: {\r\n            middlebar,\r\n            filterbar,\r\n            devicecard,\r\n            FPagination,\r\n        },\r\n\r\nsetup(){\r\n    const store = useStore();\r\n    let apidata = ref({})\r\n    let pageKey = ref(null)\r\n\r\n    const afterDataUpdate = (payload) => {\r\n        payload.dataSource;\r\n        apidata.value = payload.dataSource\r\n    }\r\n\r\n    const getPageKeyFromChild = (value) => {\r\n        pageKey.value = value\r\n    }\r\n\r\n    const axiosConfigOpts = computed(() => {\r\n        return{\r\n            url: `${window.__env.url.CN_URL}api/dashboard/device/info/`, //config\r\n            method: 'post',\r\n            data: {\r\n                field: getSearchStatus.value,\r\n                groupBy: 'date',\r\n                sort: getSortStatus.value.sortByDevId ? 'devId' : 'status',\r\n                direction: getSortStatus.value.sortByLow ? 'asc' : 'desc',\r\n            },\r\n            headers: { ContentType: 'application/json; charset=utf-8' },   \r\n        }          \r\n    })\r\n\r\n    const getSortStatus = computed(()=>{\r\n        return store.state.sortStatus;        \r\n    })\r\n\r\n    const getSearchStatus = computed(()=>{\r\n        return store.state.search;     \r\n    })\r\n\r\n    return {\r\n        apidata,\r\n        pageKey,\r\n        store,\r\n        afterDataUpdate,\r\n        getPageKeyFromChild,\r\n        axiosConfigOpts,\r\n        getSortStatus,\r\n        getSearchStatus,\r\n    }\r\n}\r\n\r\n}\r\n</script>\r\n\r\n<style >\r\n  .main{\r\n      width: 100%;\r\n      height: 600px;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      font-size: 40px;\r\n    } \r\n    body {\r\n        position: relative;\r\n        background-color: #F5F9FC;\r\n        font-family: 'Noto Sans TC','微軟正黑體','Microsoft JhengHei',Arial,sans-serif;\r\n        color: #23595E;\r\n    }\r\n    body .container-fluid-main {\r\n        max-width: 1366px;\r\n    }\r\n    body .tertiary-color {\r\n        color: #4ACBD6;\r\n    }\r\n    .wrapper{\r\n        padding: 0 15px;\r\n    }\r\n    .container-fluid {\r\n        padding-top: 10px;\r\n        margin: 16px 0;\r\n    }\r\n\r\n    .btn-primary{\r\n        background-color: #18a689;\r\n        border-color: #18a689;\r\n    }\r\n    .btn-primary:hover{\r\n        background-color: #198f77;\r\n        border-color: #198f77; \r\n    }\r\n    .card-effect {\r\n        position: relative;\r\n        border: none;\r\n        -webkit-box-shadow: 0 1px 3px rgb(93 169 189 / 24%), 0 1px 2px rgb(93 169 189 / 24%);\r\n        box-shadow: 0 1px 3px rgb(93 169 189 / 24%), 0 1px 2px rgb(93 169 189 / 24%);\r\n        -webkit-transition: all 0.3s cubic-bezier(0.25, 0.8, 0.25, 1);\r\n        transition: all 0.3s cubic-bezier(0.25, 0.8, 0.25, 1);\r\n    }\r\n    .card-effect:hover {\r\n        -webkit-box-shadow: 0 14px 28px rgb(96 185 209 / 24%), 0 10px 10px rgb(93 169 189 / 24%);\r\n        box-shadow: 0 14px 28px rgb(96 185 209 / 24%), 0 10px 10px rgb(93 169 189 / 24%);\r\n    }\r\n</style>\r\n\r\n"]}]}