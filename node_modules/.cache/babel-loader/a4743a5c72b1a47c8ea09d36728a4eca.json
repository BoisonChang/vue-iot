AC,SAAD,EAAYyC,GAAG,CAAC7D,IAAJ,CAASA,IAArB,CAAN;AAFyB,kDAGlB6D,GAAG,CAAC7D,IAAJ,CAASA,IAHS;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAI1B,KAVI;AAWCgE,IAAAA,UAXD,6BAWqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAR5C,gBAAAA,MAAQ,SAARA,MAAQ;AAAA;AAAA,uBACNvD,GAAG,CAACoG,6BAAJ,EADM;;AAAA;AAClBJ,gBAAAA,GADkB;AAExBzC,gBAAAA,MAAM,CAAC,QAAD,EAAWyC,GAAG,CAAC7D,IAAJ,CAASA,IAApB,CAAN;AAFwB,kDAGjB6D,GAAG,CAAC7D,IAAJ,CAASA,IAHQ;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIzB,KAfI;AAgBCkE,IAAAA,QAhBD,2BAgB4B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAjBC,gBAAAA,QAAiB,SAAjBA,QAAiB,EAAPrG,KAAO,SAAPA,KAAO;;AAAA,qBAC5BA,KAAK,CAACgB,WAAN,CAAkB8D,MADU;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEvBuB,QAAQ,CAAC,SAAD,EAAYrG,KAAK,CAACc,QAAlB,CAFe;;AAAA;AAAA;AAAA;;AAAA;AAAA,oBAIpBd,KAAK,CAACgB,WAAN,CAAkB8D,MAJE;AAAA;AAAA;AAAA;;AAK7BhC,gBAAAA,OAAO,CAACC,GAAR,CAAY,GAAZ;AAL6B;AAAA,uBAMvBsD,QAAQ,CAAC,SAAD,EAAYrG,KAAK,CAACc,QAAlB,CANe;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAShC,KAzBI;AA0BCwF,IAAAA,OA1BD,0BA0BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAPtG,gBAAAA,KAAO,SAAPA,KAAO;AAAA;AAAA,uBACdD,GAAG,CAACwG,sBAAJ,CAA2BvG,KAAK,CAACwB,cAAjC,CADc;;AAAA;AAEpBsB,gBAAAA,OAAO,CAACC,GAAR,CAAY,aAAZ;;AAFoB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGrB,KA7BI;AA8BCyD,IAAAA,OA9BD,0BA8BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAPxG,gBAAAA,KAAO,SAAPA,KAAO;AACpB8C,gBAAAA,OAAO,CAACC,GAAR,CAAY/C,KAAK,CAACgB,WAAlB;AADoB;AAAA,uBAEdjB,GAAG,CAAC0G,2BAAJ,CAAgC;AAACvE,kBAAAA,IAAI,EAAElC,KAAK,CAACgB;AAAb,iBAAhC,CAFc;;AAAA;AAGpBhB,gBAAAA,KAAK,CAACgB,WAAN,GAAoB,EAApB;;AAHoB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIrB;AAlCI,GAzQgB;AA6SzB0F,EAAAA,OAAO,EAAE;AA7SgB,CAAD,CAA1B","sourcesContent":["import { createStore } from 'vuex'\r\nimport * as api from '@/utils/api.js'\r\n\r\nexport default createStore({\r\n  state: {\r\n    sortStatus: {\r\n        sortByDevId: true,\r\n        sortByStatus: false,\r\n        sortByLow: true, \r\n    },\r\n    search: '',\r\n    ERR: false,\r\n    RUL: false,\r\n    MA: false,\r\n    msgType: 'ERR',\r\n    msgTypeTemp: 'ERR',\r\n    msgTypeStatus: false,\r\n    msgLang: 'zh-TW',\r\n    msgUseId: 0,\r\n    msgDelId: 0,\r\n    msgDelIdTemp: 0,\r\n    msgDelIdAPI: [],\r\n    msgAdd: {\r\n      name: '',\r\n      title: '',\r\n      content: [],\r\n      sendTypeList: \"[\\\"MAIL\\\"]\"\r\n    },\r\n    msgChanged: false,\r\n    msgChangedTemp: false,\r\n    msgChangedSave: false,\r\n    delModal: false,\r\n    addModal: false,\r\n    editStatus: false,\r\n    cancelcheck: false,\r\n    toolbtn: {\r\n      edit: true,\r\n      delete: true,\r\n      save: false,\r\n      cancel: false,\r\n    },\r\n    data: []\r\n  },\r\n  getters: {\r\n    sampleMsg(state){\r\n      if(state.search != false){\r\n        return false\r\n      }\r\n      return state[state.msgType]    \r\n    },\r\n    newMsg(state){\r\n      if( state[state.msgType] !== false ){\r\n        if(state.search != false){\r\n          if(state.msgDelId !== 0 || state.msgChanged !==  false){ // 如果已經有元素被刪除時，讀取刪除後的內容\r\n            return  state.msgChanged.filter( msg => {\r\n              return msg['name'].match(state.search)\r\n            })                    \r\n          }\r\n          return state[state.msgType].filter( msg => {\r\n            return msg['name'].match(state.search)\r\n          })\r\n        }\r\n        if(state.msgDelId !== 0 || state.msgChanged !==  false){\r\n          if (state.msgChanged !==  false){     // 如果已經有元素被刪除時，讀取刪除後的內容\r\n            state.msgChanged = [...state.msgChanged.filter( msg =>{ return msg['id'] != state.msgDelId })]\r\n            return state.msgChanged.slice(1)    // 保留 state.msgChanged 中 msg.msgId 為 0 的內容以便進入判斷式，但回傳時用 slice 篩選掉 msg.msgId 為 0 的範本\r\n          }\r\n          state.msgChanged = [...state[state.msgType].filter( msg =>{ return msg['id'] != state.msgDelId })]\r\n          return  state.msgChanged.slice(1)    \r\n        }\r\n        return state[state.msgType].slice(1)\r\n      }\r\n    },\r\n    showMsg(state) {\r\n      if(state[state.msgType] !== false){\r\n        return state[state.msgType].find(msg => msg['id'] > state.msgUseId - 1 )\r\n      } \r\n    }\r\n  },\r\n  mutations: {\r\n    resetDeletMsgToolBtn(state){\r\n      state.msgDelId = 0\r\n      console.log('state.msgTypeStatus:', state.msgTypeStatus)\r\n      if(state.msgTypeStatus){ \r\n        console.log('reset state.msgChanged ')\r\n        state.msgChanged = false\r\n        state.msgTypeStatus = false  \r\n      }     \r\n    },\r\n    setSortStatus(state, payload) { \r\n      state.sortStatus = {...payload}    \r\n    },\r\n    setSearchStatus(state, input) { \r\n      state.search = input\r\n    },\r\n    setMsgType(state, type){\r\n      if(state.toolbtn.edit == false ){ // 當有修改狀態下想要切換選單，不能直接切換要詢問確認因此先把切換值暫存\r\n        if(type == 'ERR'){\r\n          state.msgTypeTemp = 'ERR'\r\n        } else if(type == 'RUL'){\r\n          state.msgTypeTemp = 'RUL'\r\n        } else if(type == 'MA'){\r\n          state.msgTypeTemp = 'MA'\r\n        }\r\n        state.msgTypeStatus = true    // 辨識此次 cancel modal 之觸發是由 msgType 改變造成的。\r\n        state.cancelcheck = true     // 觸發 cancel modal ，但要整個執行完才觸發！！！！\r\n      } else {\r\n        if(type == 'ERR'){\r\n          state.msgType = 'ERR'\r\n        } else if(type == 'RUL'){\r\n          state.msgType = 'RUL'\r\n        } else if(type == 'MA'){\r\n          state.msgType = 'MA'\r\n        }\r\n        this.commit('setToolBtn', 'save')   // 重置上方功能按鈕\r\n      }\r\n      state.msgUseId = state[state.msgType][0].id    // 切換類別時重置選取為範本\r\n      state.msgLang = 'zh-TW'                                 // 切換類別時重置語言為繁體中文\r\n    },\r\n    cancelConfirm(state, answer){       // cancel modal 被觸發\r\n      if(answer == 'yes'){\r\n        console.log('1')\r\n        state.msgType = state.msgTypeTemp\r\n        console.log('2')\r\n        state.msgUseId = state[state.msgType][0]['id']\r\n        console.log('1')\r\n        if(state.msgChangedSave !== false){\r\n          state.msgChanged = state.msgChangedSave.map(n=>n)\r\n        }\r\n        state.editStatus = false\r\n\r\n        this.commit('resetDeletMsgToolBtn')     // 確定不儲存資料，觸發 resetDeletMsgToolBtn 將暫存 msgDelId  state.msgChanged 清空\r\n        this.commit('setToolBtn', 'save')   // 重置上方功能按鈕\r\n      } else if(answer == 'no'){\r\n        state.cancelcheck = false\r\n      }\r\n    },\r\n    setMsgLang(state, lang){\r\n      if(lang == 'zh-TW'){\r\n        state.msgLang = 'zh-TW'\r\n      } else if(lang == 'zh-CN'){\r\n        state.msgLang = 'zh-CN'\r\n      } else if(lang == 'en'){\r\n        state.msgLang = 'en'\r\n      }\r\n    },\r\n    setMsgId(state, id){\r\n      state.msgUseId = id\r\n      state.msgLang = 'zh-TW' // 重置語言\r\n    },\r\n    setToolBtn(state, click){\r\n      if(click == 'edit'){\r\n        state.cancelcheck = false\r\n        state.toolbtn.edit = false     \r\n        state.toolbtn.delete = false\r\n        state.toolbtn.cancel = true\r\n        if(state.msgUseId == state[state.msgType][0]['id']) {\r\n          state.toolbtn.save = false \r\n        } else {\r\n          state.toolbtn.save = true \r\n        }\r\n        state.editStatus = true\r\n      } else if(click == 'delete'){\r\n        state.cancelcheck = false\r\n        state.toolbtn.edit = false\r\n        state.toolbtn.delete = false\r\n        state.toolbtn.save = true \r\n        state.toolbtn.cancel = true\r\n      } else if(click == 'save'){      // 此處之 save 不會儲存資料主要改樣式跟 state，儲存資料的 mutation 分開到 saveMsg 處理\r\n        state.toolbtn.edit = true\r\n        state.toolbtn.delete = true\r\n        state.toolbtn.save = false\r\n        state.toolbtn.cancel = false\r\n        state.editStatus = false\r\n      } else if(click == 'cancel'){\r\n        state.cancelcheck = true       // 觸發 cancel modal\r\n      }\r\n    },\r\n    setSendTypeList(state){\r\n      if(state.msgAdd.sendTypeList === \"[]\"){\r\n        state.msgAdd.sendTypeList = \"[\\\"MAIL\\\"]\"\r\n      } else if (state.msgAdd.sendTypeList === \"[\\\"MAIL\\\"]\"){\r\n        state.msgAdd.sendTypeList = \"[]\"\r\n      }\r\n    },\r\n    deletMsg(state, deleteId){\r\n      state.delModal = true\r\n      state.msgDelIdTemp = deleteId\r\n    },\r\n    deleteConfirm(state, answer){\r\n      if(answer == 'yes'){\r\n        state.msgDelIdAPI.push( state[state.msgType].find(msg => msg['id'] >  state.msgDelId - 1 ).notifyMsgId )\r\n        state.msgDelId = state.msgDelIdTemp\r\n        state.delModal = false\r\n      } else if(answer == 'no'){\r\n        state.delModal = false\r\n      }\r\n    },\r\n    openAddModal(state){\r\n      state.addModal = true\r\n    },\r\n    addMsgSaveBody(state, msg){\r\n      state.msgAdd.name = msg.name || \"\"\r\n      state.msgAdd.title = msg.title || \"\"\r\n      state.msgAdd.content = msg.content || \"\"\r\n    },\r\n    addMsgSaveName(state, name){\r\n      state.msgAdd.name = name\r\n    },\r\n    addConfirm(state, answer){\r\n      if(answer == 'yes'){\r\n\r\n        if (state.msgChanged ==  false){  // 如果已經沒有元素被刪除時，讀取原本\r\n          state.msgChanged = [...state[state.msgType]]\r\n        }\r\n        // eslint-disable-next-line no-unused-vars\r\n        const { customer, gmtCreate, gmtModified, id, ...msgChangedTemp} \r\n        = state.msgChanged.find(msg => msg['id'] > state.msgUseId - 1 ) \r\n        let notifyMsgId = state[state.msgType][state[state.msgType].length - 1].notifyMsgId\r\n        let newNotifyMsgId = Number(notifyMsgId.replace(/[^0-9]/g, \"\")) + 1\r\n        let newNotifyMsgIdStr = `test${newNotifyMsgId.toString().padStart(27,'0')}`\r\n        let msgLangCode = 0\r\n        switch (state.msgLang) {\r\n          case 'zh-TW':\r\n            state.msgLangCode = 0\r\n            break;\r\n          case 'zh-CN':\r\n            state.msgLangCode = 1\r\n            break;\r\n          case 'en':\r\n            state.msgLangCode = 2\r\n            break;\r\n        }\r\n\r\n        // eslint-disable-next-line no-unused-vars\r\n        msgChangedTemp.i18nList = msgChangedTemp.i18nList.map(({gmtCreate, gmtModified, id, ...keepAttrs}) => keepAttrs)\r\n        console.log( msgChangedTemp )\r\n        msgChangedTemp.i18nList.map( type => { type.content = \"\" })\r\n\r\n        console.log('msgChangedTempDE&ADD', msgChangedTemp)\r\n        msgChangedTemp.notifyMsgId = newNotifyMsgIdStr\r\n        msgChangedTemp.name = state.msgAdd.name\r\n        msgChangedTemp.i18nList[msgLangCode]['content'] = state.msgAdd.content\r\n        msgChangedTemp.i18nList[msgLangCode]['title'] = state.msgAdd.title\r\n        msgChangedTemp.typeId = state.msgType\r\n        msgChangedTemp.sendTypeList = state.msgAdd.sendTypeList\r\n        state.msgChanged.push({...msgChangedTemp})\r\n        state.msgChangedSave = {...msgChangedTemp}\r\n        state.msgAdd.name = ''\r\n        state.addModal = false\r\n\r\n      } else if(answer == 'no'){\r\n        state.addModal = false\r\n      }\r\n      this.commit('setToolBtn', 'save')   // 重置上方功能按鈕\r\n    },\r\n    initERR(state, payload){\r\n      state.ERR = payload\r\n      state.msgUseId = state[state.msgType][0].id\r\n      console.log('state.ERR:', state.ERR)\r\n    },\r\n    initRUL(state, payload){\r\n      state.RUL = payload\r\n    },\r\n    initMA(state, payload){\r\n      state.MA = payload\r\n    },\r\n  },\r\n  actions: { // 呼叫 api\r\n      async handInitERR({commit}){\r\n        const res = await api.apiGetNotifyMessageByTypeIdERR()\r\n        commit('initERR', res.data.data)     \r\n        return res.data.data\r\n      },\r\n      async handInitRUL({commit}){\r\n        const res = await api.apiGetNotifyMessageByTypeIdRUL()\r\n        commit('initRUL', res.data.data)     \r\n        return res.data.data\r\n      },\r\n      async handInitMA({commit}){\r\n        const res = await api.apiGetNotifyMessageByTypeIdMA()\r\n        commit('initMA', res.data.data)     \r\n        return res.data.data\r\n      },\r\n      async handSave({dispatch, state}){\r\n        if(state.msgDelIdAPI.length){\r\n          await dispatch('handDel', state.msgDelId)\r\n          // await dispatch(`handInit${state.msgType}`)\r\n        } else if(!state.msgDelIdAPI.length){\r\n          console.log('3')\r\n          await dispatch('handAdd', state.msgDelId)\r\n          // await dispatch(`handInit${state.msgType}`)\r\n        }\r\n      },\r\n      async handAdd({state}){\r\n        await api.apiInsertNotifyMessage(state.msgChangedSave)\r\n        console.log('finish add?')     \r\n      },\r\n      async handDel({state}){\r\n        console.log(state.msgDelIdAPI)\r\n        await api.apiDeleteNotifyMessageBatch({data: state.msgDelIdAPI})    \r\n        state.msgDelIdAPI = [] \r\n      }      \r\n  },\r\n  modules: {\r\n  }\r\n})\r\n\r\n\r\n"]}]}