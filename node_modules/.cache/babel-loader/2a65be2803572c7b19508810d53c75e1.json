\r\n    low.value = true\r\n}\r\nconst resetOrder = () => {\r\n    devId.value = true\r\n    status.value = false\r\n    low.value = true \r\n}\r\nconst keepNowOrder = () => {\r\n    devId.value = devIdSave.value\r\n    status.value = statusSave.value\r\n    low.value = lowSave.value\r\n}\r\nconst saveNewOrder = () => {\r\n    devIdSave.value =  devId.value\r\n    statusSave.value = status.value\r\n    lowSave.value = low.value\r\n}\r\nwatch(props.originaldata, () => {\r\n    apidata.value = props.originaldata.value\r\n})\r\nonMounted(()=>{\r\n    nextTick(function() {\r\n        document.addEventListener(\"click\", e => { \r\n            detectClick(e);    \r\n        });\r\n    });   \r\n})\r\n\r\n"]}]}