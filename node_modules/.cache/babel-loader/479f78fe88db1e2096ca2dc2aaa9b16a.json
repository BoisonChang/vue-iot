or/ckeditor5-build-classic/build/translations/pt';\r\n\r\n\nexport default {\n  setup(__props, { expose }) {\n  expose();\n\r\nconst store = useStore();\r\nconst editor = ref(Editor);\r\nconst editorContent = ref('');\r\nconst editorConfig = ref({\r\n    toolbar: {\r\n        items: [\r\n            'heading',\r\n            '|',                                                // <--- 此处为新增\r\n            'bold',\r\n            'italic',\r\n            'underLine',\r\n            'bulletedList',\r\n            'numberedList',\r\n            'removeFormat',\r\n            'TextColor',\r\n        ],\r\n    },\r\n});\r\nconst editTitle = ref('');\r\nconst uneditContentTop = ref('')\r\nconst uneditContentFooter = ref('')\r\n\r\nconst switchMsgLang = (lang) => {\r\n  if(store.state.editStatus && store.state.saveStatus){\r\n    addMsgSaveBody({title: editTitle, content: editorContent, contentTop: uneditContentTop, contentBottom: uneditContentFooter})\r\n  }\r\n  store.commit('setMsgLang', lang);\r\n};\r\n\r\nconst openModal = () => {\r\n  addMsgSaveBody({title: editTitle, content: editorContent, contentTop: uneditContentTop, contentBottom: uneditContentFooter})\r\n  store.commit('openAddModal');\r\n};\r\n\r\nconst switchSendTypeList = () => {\r\n  store.commit('setSendTypeList');\r\n};\r\n\r\nconst addMsgSaveBody = (msgBody) => {\r\n  store.commit('addMsgSaveBody', msgBody);\r\n};\r\n\r\nconst getPartFromApiData = (part, string) => {\r\n   if (store.getters.showMsg?.i18nList) {\r\n    const apiData = store.getters.showMsg?.i18nList.filter((item) => item.lang === store.state.msgLang)[0].content;\r\n    const parser = new DOMParser().parseFromString(apiData, 'text/html');\r\n    const apiDataContent = parser.getElementsByClassName(string)[0]?.innerHTML;\r\n    part.value = apiDataContent || ' ';\r\n  }\r\n}\r\n\r\nwatchEffect(() => {\r\n  if(store.state.msgAdd.content[store.state.msgLang] || store.state.msgAdd.title[store.state.msgLang]){\r\n    editorContent.value = store.state.msgAdd.content[store.state.msgLang]\r\n    editTitle.value = store.state.msgAdd.title[store.state.msgLang]\r\n  } else {\r\n    getPartFromApiData(editorContent, 'click2edit')\r\n    getPartFromApiData(uneditContentTop, 'uneditable')\r\n    getPartFromApiData(uneditContentFooter, 'msg-footer')\r\n    editTitle.value = store.getters.showMsg?.i18nList.filter((item) => item.lang === store.state.msgLang)[0].title;\r\n  }\r\n});\r\n\r\nwatch(\r\n  ()=> store.state.saveStatus,\r\n  ()=> {\r\n    if(store.state.saveStatus){\r\n      addMsgSaveBody({title: editTitle, content: editorContent, contentTop: uneditContentTop, contentBottom: uneditContentFooter})\r\n      store.dispatch('handSave')\r\n    }\r\n  }\r\n)\r\n\r\n\nconst __returned__ = { store, editor, editorContent, editorConfig, editTitle, uneditContentTop, uneditContentFooter, switchMsgLang, openModal, switchSendTypeList, addMsgSaveBody, getPartFromApiData, useStore, ref, watch, watchEffect, Editor }\nObject.defineProperty(__returned__, '__isScriptSetup', { enumerable: false, value: true })\nreturn __returned__\n}\n\n}"]}]}