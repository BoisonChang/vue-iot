          \"msgSolution\": \"解決方式 \"\r\n        }\r\n      ],\r\n      \"ZhCN\": [  \r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"錯誤代碼通知預設範本\",\r\n          \"msgTitle\": \"[错误代码] 异常警告通知\",\r\n          \"msgProductId\": \"裝置識別碼\",\r\n          \"msgContentCode\": \"代码\",\r\n          \"msgProblem\": \"問題等級\",\r\n          \"msgReason\": \"发生原因\",\r\n          \"msgSolution\": \"解决方式\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"1\",\r\n            \"msgName\": \"錯誤代碼藍\",\r\n            \"msgTitle\": \"[错误代码] 异常警告通知....\",\r\n            \"msgProductId\": \"裝置識別碼\",\r\n            \"msgContentCode\": \"代码\",\r\n            \"msgProblem\": \"問題等級\",\r\n            \"msgReason\": \"发生原因\",\r\n            \"msgSolution\": \"解决方式\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"2\",\r\n            \"msgName\": \"0826\",\r\n            \"msgTitle\": \"[错误代码] 异常警告通知\",\r\n            \"msgProductId\": \"裝置識別碼\",\r\n            \"msgContentCode\": \"代码\",\r\n            \"msgProblem\": \"問題等級\",\r\n            \"msgReason\": \"发生原因\",\r\n            \"msgSolution\": \"解决方式\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"3\",\r\n            \"msgName\": \"111\",\r\n            \"msgTitle\": \"[错误代码] 异常警告通知\",\r\n            \"msgProductId\": \"裝置識別碼\",\r\n            \"msgContentCode\": \"代码\",\r\n            \"msgProblem\": \"問題等級\",\r\n            \"msgReason\": \"发生原因\",\r\n            \"msgSolution\": \"解决方式\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"4\",\r\n            \"msgName\": \"123\",\r\n            \"msgTitle\": \"[错误代码] 异常警告通知\",\r\n            \"msgProductId\": \"裝置識別碼\",\r\n            \"msgContentCode\": \"代码\",\r\n            \"msgProblem\": \"問題等級\",\r\n            \"msgReason\": \"发生原因\",\r\n            \"msgSolution\": \"解决方式\"\r\n        },   \r\n      ],\r\n      \"EN\": [\r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"錯誤代碼通知預設範本\",\r\n          \"msgTitle\": \"[Error code] Warning: An Exceptional Problem Occurred.\",\r\n          \"msgProductId\": \"identifier\",\r\n          \"msgContentCode\": \"Code\",\r\n          \"msgProblem\": \"Level\",\r\n          \"msgReason\": \"Cause\",\r\n          \"msgSolution\": \"Solution\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"1\",\r\n            \"msgName\": \"錯誤代碼藍\",\r\n            \"msgTitle\": \"[Error code] Warning: An Exceptional Problem Occurred.\",\r\n            \"msgProductId\": \"identifier\",\r\n            \"msgContentCode\": \"Code\",\r\n            \"msgProblem\": \"Level\",\r\n            \"msgReason\": \"Cause\",\r\n            \"msgSolution\": \"Solution\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"2\",\r\n            \"msgName\": \"0826\",\r\n            \"msgTitle\": \"[Error code] Warning: An Exceptional Problem Occurred.\",\r\n            \"msgProductId\": \"identifier\",\r\n            \"msgContentCode\": \"Code\",\r\n            \"msgProblem\": \"Level\",\r\n            \"msgReason\": \"Cause\",\r\n            \"msgSolution\": \"Solution\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"3\",\r\n            \"msgName\": \"111\",\r\n            \"msgTitle\": \"[Error code] Warning: An Exceptional Problem Occurred.\",\r\n            \"msgProductId\": \"identifier\",\r\n            \"msgContentCode\": \"Code\",\r\n            \"msgProblem\": \"Level\",\r\n            \"msgReason\": \"Cause\",\r\n            \"msgSolution\": \"Solution\"\r\n        }, \r\n        {   \r\n            \"msgId\": \"4\",\r\n            \"msgName\": \"123\",\r\n            \"msgTitle\": \"[Error code] Warning: An Exceptional Problem Occurred.\",\r\n            \"msgProductId\": \"identifier\",\r\n            \"msgContentCode\": \"Code\",\r\n            \"msgProblem\": \"Level\",\r\n            \"msgReason\": \"Cause\",\r\n            \"msgSolution\": \"Solution\"\r\n        },    \r\n      ]\r\n    },\r\n    Rule: {\r\n      \"ZhTW\": [\r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"規則通知預設範本\",\r\n          \"msgTitle\": \"[規則庫] 警告通知\",\r\n          \"msgProductId\": \"裝置識別碼\",\r\n          \"msgProduct\": \"感測器\",\r\n          \"msgRule\":\"規則\",\r\n          \"msgNumber\":\"觸發數值 \"      \r\n        }, \r\n      ],\r\n      \"ZhCN\":[\r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"規則通知預設範本\",\r\n          \"msgTitle\": \"[规则库] 警告通知\",\r\n          \"msgProductId\": \"装置识别码\",\r\n          \"msgProduct\": \"感测器\",\r\n          \"msgRule\":\"规则\",\r\n          \"msgNumber\":\"触发数值\"      \r\n        }, \r\n      ],\r\n      \"EN\":[\r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"規則通知預設範本\",\r\n          \"msgTitle\": \"[Rule Base] Warning\",\r\n          \"msgProductId\": \"identifier\",\r\n          \"msgProduct\": \"property\",\r\n          \"msgRule\":\"Rule\",\r\n          \"msgNumber\":\"Payload\"      \r\n        }, \r\n      ]\r\n    },\r\n    Keep: {\r\n      \"ZhTW\": [\r\n        {   \r\n            \"msgId\": \"0\",\r\n            \"msgName\": \"維護保養通知預設範本\",\r\n            \"msgTitle\": \"[維護保養] 提醒通知\",\r\n            \"msgProductId\": \"裝置識別碼\",\r\n            \"msgProduct\": \"感測器\",\r\n            \"msgType\":\"維護類型\",\r\n            \"msgMethod\":\"維護方式\"              \r\n        }, \r\n      ],\r\n      \"ZhCN\": [\r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"維護保養通知預設範本\",\r\n          \"msgTitle\": \"[维护保养] 提醒通知\",\r\n          \"msgProductId\": \"装置识别码\",\r\n          \"msgProduct\": \"感测器\",\r\n          \"msgType\":\"维护类型\",\r\n          \"msgMethod\":\"維護方式\"              \r\n        }, \r\n      ],\r\n      \"EN\": [\r\n        {   \r\n          \"msgId\": \"0\",\r\n          \"msgName\": \"維護保養通知預設範本\",\r\n          \"msgTitle\": \"[Maintenance] Maintenance Reminder\",\r\n          \"msgProductId\": \"identifier\",\r\n          \"msgProduct\": \"property\",\r\n          \"msgType\":\"MaintenanceType\",\r\n          \"msgMethod\":\"維護方式\"              \r\n        }, \r\n      ]\r\n    },\r\n    msgType: 'Error',\r\n    msgLang: 'ZhTW',\r\n    msgUseId: 0,\r\n    msgdeleteId: 0,\r\n    msgdeleteIdTemp: 0,\r\n    msgdeletedSave: false,\r\n    deletemodal: false,\r\n    toolbtn: {\r\n      edit: true,\r\n      delete: true,\r\n      save: false,\r\n      cancel: false,\r\n    }\r\n  },\r\n  getters: {\r\n    baseMsg(state) {\r\n      if(state.search != false){ \r\n        return  false\r\n      }\r\n      return state[state.msgType][state.msgLang].filter(msg => msg.msgId < 1)\r\n    },\r\n    newMsg(state) {\r\n      if(state.search != false){ \r\n        if(state.msgdeleteId !== 0){ // 如果已經有元素被刪除時，讀取刪除後的內容\r\n          return  state.msgdeletedSave.filter( msg => msg.msgId > 0).filter( msg => {\r\n            return msg.msgName.match(state.search)\r\n          })        \r\n        }\r\n        return  state[state.msgType][state.msgLang].filter( msg => {\r\n          return msg.msgName.match(state.search)\r\n        })\r\n      }\r\n      if(state.msgdeleteId !== 0){ // 如果已經有元素的 id 被加入 state.msgdeleteId 時\r\n        if (state.msgdeletedSave !=  false){  // 如果已經有元素被刪除時，讀取刪除後的內容\r\n          state.msgdeletedSave = state.msgdeletedSave.filter( msg =>{ return msg.msgId != state.msgdeleteId })\r\n          return state.msgdeletedSave.filter( msg => msg.msgId > 0 )  // 保留 state.msgdeletedSave 中 msg.msgId 為 0 的內容以便進入判斷式，但回傳時用 filter 篩選掉 msg.msgId 為 0 的範本\r\n        }\r\n        state.msgdeletedSave = state[state.msgType][state.msgLang].filter( msg =>{ return msg.msgId != state.msgdeleteId })\r\n        return  state.msgdeletedSave.filter( msg => msg.msgId > 0 )\r\n      }\r\n      return state[state.msgType][state.msgLang].filter(msg => msg.msgId > 0)\r\n    },\r\n    showMsg(state) {\r\n      return state[state.msgType][state.msgLang].find(msg => msg.msgId > state.msgUseId - 1  )\r\n    }\r\n  },\r\n  mutations: {\r\n    resetDeletMsgToolBtn(state){\r\n      state.msgdeleteId = 0\r\n      state.msgdeletedSave = false\r\n      this.commit('setToolBtn', 'cancel')  \r\n    },\r\n    setSortStatus(state, payload) { \r\n      state.sortStatus = {...payload}    \r\n    },\r\n    setSearchStatus(state, payload) { \r\n      state.search = payload\r\n    },\r\n    setMsgType(state, payload){\r\n      if(payload == 'Error'){\r\n        state.msgType = 'Error'\r\n      } else if(payload == 'Rule'){\r\n        state.msgType = 'Rule'\r\n      } else if(payload == 'Keep'){\r\n        state.msgType = 'Keep'\r\n      }\r\n      this.commit('resetDeletMsgToolBtn')\r\n    },\r\n    setMsgLang(state, payload){\r\n      if(payload == 'ZhTW'){\r\n        state.msgLang = 'ZhTW'\r\n      } else if(payload == 'ZhCN'){\r\n        state.msgLang = 'ZhCN'\r\n      } else if(payload == 'EN'){\r\n        state.msgLang = 'EN'\r\n      }\r\n    },\r\n    setMsgId(state, payload){\r\n      state.msgUseId = payload\r\n    },\r\n    setToolBtn(state, payload){\r\n      if(payload == 'edit'){\r\n        state.toolbtn.edit = false\r\n        state.toolbtn.delete = false\r\n        state.toolbtn.cancel = true\r\n        if(state.msgUseId == 0) {\r\n          state.toolbtn.save = false \r\n        } else {\r\n          state.toolbtn.save = true \r\n        }\r\n      } else if(payload == 'delete'){\r\n        state.toolbtn.edit = false\r\n        state.toolbtn.delete = false\r\n        state.toolbtn.save = true \r\n        state.toolbtn.cancel = true\r\n      } else if(payload == 'save'){\r\n        state.toolbtn.edit = true\r\n        state.toolbtn.delete = true\r\n        state.toolbtn.save = false\r\n        state.toolbtn.cancel = false\r\n      } else if(payload == 'cancel'){\r\n        state.toolbtn.edit = true\r\n        state.toolbtn.delete = true\r\n        state.toolbtn.save = false\r\n        state.toolbtn.cancel = false\r\n      }\r\n    },\r\n    deletMsg(state, deleteId){\r\n      state.deletemodal = 