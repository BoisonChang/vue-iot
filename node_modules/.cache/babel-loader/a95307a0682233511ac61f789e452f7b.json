  store.commit('setStateObject', {'key1': 'msgAdd', 'key2': 'sendTypeList' , 'value':  '[]'});\r\n  }\r\n};\r\n\r\nconst addMsgSaveBody = (msgBody) => {\r\n  store.commit('addMsgSaveBody', msgBody);\r\n};\r\n\r\nconst getPartFromApiData = (part, string) => {\r\n   if (store.getters.showMsg?.i18nList) {\r\n    const apiData = store.getters.showMsg?.i18nList.filter((item) => item.lang === store.state.msgLang)[0].content;\r\n    const parser = new DOMParser().parseFromString(apiData, 'text/html');\r\n    const apiDataContent = parser.getElementsByClassName(string)[0]?.innerHTML;\r\n    part.value = apiDataContent || ' ';\r\n  }\r\n}\r\n\r\nwatchEffect(() => { \r\n  console.log(store.state.editStatus)\r\n  if(store.state.msgAdd.content[store.state.msgLang] || store.state.msgAdd.title[store.state.msgLang]){\r\n    editorContent.value = store.state.msgAdd.content[store.state.msgLang]\r\n    editTitle.value = store.state.msgAdd.title[store.state.msgLang]\r\n    console.log('A')\r\n  } else {\r\n    getPartFromApiData(editorContent, 'click2edit')\r\n    getPartFromApiData(uneditContentTop, 'uneditable')\r\n    getPartFromApiData(uneditContentFooter, 'msg-footer')\r\n    editTitle.value = store.getters.showMsg?.i18nList.filter((item) => item.lang === store.state.msgLang)[0].title;\r\n    console.log('B')\r\n  }\r\n});\r\n\r\nwatch(\r\n  ()=> store.state.saveStatus,\r\n  ()=> {\r\n    if(store.state.saveStatus){\r\n      addMsgSaveBody({title: editTitle, content: editorContent, contentTop: uneditContentTop, contentBottom: uneditContentFooter})\r\n      store.dispatch('handSave')\r\n    }\r\n  }\r\n)\r\n\r\n\nconst __returned__ = { store, editor, editorContent, editorConfig, editTitle, uneditContentTop, uneditContentFooter, switchMsgLang, openModal, switchSendTypeList, addMsgSaveBody, getPartFromApiData, useStore, ref, watch, watchEffect, Editor }\nObject.defineProperty(__returned__, '__isScriptSetup', { enumerable: false, value: true })\nreturn __returned__\n}\n\n}"]}]}